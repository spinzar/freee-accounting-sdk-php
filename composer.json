{
    "name": "freee/freee-accounting-sdk",
    "type": "library",
    "version": "v2.0.0",
    "description": "Accounting freee PHP SDK",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "freee"
    ],
    "homepage": "https://developer.freee.co.jp/",
    "license": "MIT",
    "authors": [
        {
            "name": "Freee",
            "homepage": "https://developer.freee.co.jp/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Freee\\Accounting\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Freee\\Accounting\\" : "test/" }
    }
}
